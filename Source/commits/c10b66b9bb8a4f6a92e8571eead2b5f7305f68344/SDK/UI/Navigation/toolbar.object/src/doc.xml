        <method name="build" returntype="toolbar">
          <description>Builds the toolbar.
It returns the holder. In the future this will return the toolbar. It should not be used as a return value for building content.
Use build() and get() instead.</description>
          <parameters>
            <parameter name="$dock" type="string" defaultvalue="">
              <description>The dock position of the toolbar.
Possible values:
T - Top, Horizontal
B - Bottom, Horizontal
L - Left, Vertical
R - Right, Vertical</description>
            </parameter>
            <parameter name="$id" type="string" defaultvalue="">
              <description>The toolbar unique id.
If empty, it will get a random unique id.
It is empty by default.</description>
            </parameter>
            <parameter name="$class" type="string" defaultvalue="">
              <description>The toolbar extra class for custom styling.
It is empty by default.</description>
            </parameter>
          </parameters>
          <deprecated/>
          <returndescription>The toolbar object.</returndescription>
          <throws/>
        </method>
        <method name="insertTool" returntype="void">
          <description>Inserts an item into the toolbar.</description>
          <parameters>
